<div class="row"> 
    <gameview> 
        <div class="col-md-3 manor" ng-controller="GameCtrl"> 
            <h4>Notifications</h4>
            <div class="well">
                <ul id="notification" class="list-unstyled">
                    <li>Mathew</li> 
                </ul>
            </div>
            <h4>Anomaly List</h4>
            <visitor>
                <div ng-show="person" class="well">
                    <button type="button" class="close badge" aria-hidden="true" ng-click="close()" close-dialog>x</button>
                    <h4>{{person.name}} says...</h4>
                    <p>{{person.request}}</p>
                    <div id="dialog"></div>
                    <button class="btn btn-xs btn-primary" ng-click="inquire()">Inquire</button>
                    <button class="btn btn-xs btn-success">Accept</button>
                    <button class="btn btn-xs btn-danger">Reject</button>
                </div>

                <div ng-repeat="anomaly in anomalylist" class="panel panel-default" anomalies style="z-index:1000">
                    <div class="panel-heading" anomaly-id="{{anomaly.id}}">Report #{{anomaly.title}}</div>
                    <div class="panel-body"><a href="#" ng-click="talk(anomaly.id)" speak>process</a></div>
                    <!--<a href="#" ng-click="reject()" reject>reject...</a>-->
                </div> 

            </visitor>
        </div>
    </gameview>
